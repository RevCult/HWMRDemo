<apex:page standardController="Project__c" sidebar="false" showHeader="false" standardStylesheets="false" docType="html-5.0" extensions="BuilderProjectEditExt" cache="false" action="{!init}">
    <head>
        <title>Zonda iPad App, Real-time homebuilding data from Meyers, a Kennedy Wilson Company</title>
        <apex:stylesheet value="{!$Resource.zonda_css}" />
        
        <apex:stylesheet value="{!URLFOR($Resource.jqueryui, 'jquery-ui/jquery-ui.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.jqueryui, 'jquery-ui/jquery-ui.structure.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.jqueryui, 'jquery-ui/jquery-ui.theme.min.css')}" />
        
        <style type="text/css">
            .projectDataLabel {font-weight: normal; width: 210px; display: inline-block; vertical-align: top;}
            .projectDataLabel-na {font-weight: normal; width: 25px; display: inline-block;text-align: left; padding-right: 15px;}
            .wideInput {width: 400px;}
            .multiSelectPicklistTable {width: 400px; display: inline-block;}
            select[id$="selected"] { width: 200px; }
            input {width: 200px;}
            input[type='checkbox'] { width:15px; }
            textarea {font-family: inherit; font-size: 1em;}
            
            .section-container {clear: both;}
            .section-header {font-weight: bold; line-height: 2em;}
            .section-details {line-height: 2em; font-size: .7em;}
            .section-details .left-col {width: 49%; float: left; }
            .section-details .right-col {width: 49%; float: left; }
            .section-details .single-col {width: 100%; clear: both;}
            
            .section-submit-button { clear: both; width: 100%; margin: 0 auto; margin-top: 50px; text-align: center; }
            
            .dateFormat {display: none;}
            .datePicker {font-size: .8em;}
            
            .hidden {visibility: hidden;}
            
            .message {background: red; padding: 10px; font-size: .7em; color: #fff;}
            .message h4 {color: #fff; font-size: 1.2em;}
            .error-msg {color: #dc3737;}
            
            /* The Modal (background) */
            .modal {
              display: none; /* Hidden by default */
              position: fixed; /* Stay in place */
              z-index: 1; /* Sit on top */
              left: 0;
              top: 0;
              width: 100%; /* Full width */
              height: 100%; /* Full height */              
              overflow: auto; /* Enable scroll if needed */
              background-color: rgb(0,0,0); /* Fallback color */
              background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
            }
            
            /* Modal Content */
            .modal-content {
            border-radius: 3px;
            background-color: #ffb75d;
            margin: auto;
            padding: 15px;
            border: 1px solid #ffb75d;
            position: fixed;
            top: 100px;
            left: 0;
            right: 0;
            max-width: 500px;
            }
            
            /* The Close Button */
            .close {
              color: #080707;
              float: right;
              font-size: 28px;
              font-weight: bold;
              margin-top: -15px;
              margin-right: -4px !important;
            }
            
            .warning-smb {
              color: #514f4d;
              float: left;
              font-size: 50px;
              font-weight: bold;
              margin-top: -13px;
              psdding-right: 5px;   
            }
            
            .close:hover,
            .close:focus {
              color: #514f4d;
              text-decoration: none;
              cursor: pointer;
            }
            
            h2#message-content {
            color: #4a3c3c;
            }
            
            .inline-element{
            display: inline-block;
            width: 49%;
            }
            .inline-element-3-col{
            display: inline-block;
            width: 70% !important;
            }
            .chkbox-3-col{
            width: 32% !important;
            }
            .naTxt{
            display:none;
            }
            .insentive-txt{
            width : 55%;
            overflow-y: scroll;
            word-wrap: break-word;
            word-break: break-all;
            rows="5"
            }
            
        </style>
        
        <!-- There's a bunch of javascript at the bottom of this page -->
    </head>
    
    <header>
        <div class="container">
