    <script type="text/javascript">
    var allowSubmit = true;
    var inValidElements = [];
    var currentPopedUp = '';
    $j(document).ready(function() {
        
        $j('.priceField').on('blur', function() {
            handlePriceFieldFormat(this);
        }).each(function() {
            handlePriceFieldFormat(this);
        });
    });
    
    var handlePriceFieldFormat = function(theField) {
        if (theField.value.length > 0) {
            theField.value=parseInt((theField.value).toString().replace(/,|\$/g,'')).toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,") || '';
        }
    }
    
    // Checking valition before saving 
    function isValidToSave(event) {
        if(!allowSubmit){
            event.preventDefault();
            makeFieldsBlank(inValidElements);
            return false;
        }else {
            return true;
        }
    }
    
    // Making fields blank without removing error message 
    function makeFieldsBlank(elments){
        if(elments){
            elments.forEach(function(element) {
                element.value = '';
                //removeErrorMessage($j(element).parent().parent().next('.error-msg'));
                element.focus();
            });
        }
    }

    //show popup message to user
    function showPopupMessage(fieldname, element){
        var msg = createErrorMessage(fieldname, ' ');
        if(element.value){
            if(element.value.length > 110){
                $j('#message-content').html( msg );
                $j('#msgModal').css("display", "block");   
                currentPopedUp = element;
            }
        }
    }
    
    // Create the error message 
    function createErrorMessage(fieldname, extraLine){
        return "The maximum allowable characters for <strong>"+fieldname+"</strong> is 110 characters long."+extraLine+"Please edit the content and save then submit record.";
    }

    // To show the error messages for intensive
    function showCharUsageAlert(fieldname, element, parentDiv) {            
        var str = element.value;
        var nextElements = $j(element).parent().parent().nextAll( '.error-msg' );
        if(str){
            var strLen = str.length;   
            // if character length goes above 110 chars
            if(strLen > 110){
                if(nextElements.length === 0){                       
                    var msg = createErrorMessage(fieldname, ' ');
                    displayErrorMessage(msg, element);
                    inValidElements.push(element);
                }
            }else{
                if(nextElements.length > 0){
                    removeElementFromInvalidElementsArray(element);
                    removeErrorMessage($j(element).parent().parent().next('.error-msg'));
                }
            }
        }else{
            if(nextElements.length > 0){
                removeErrorMessage($j(element).parent().parent().next('.error-msg'));
                removeElementFromInvalidElementsArray(element);                
            }
        }        
        setAllowSubmission();
    }
    
    // Display the error message.
    function displayErrorMessage(errorMessage, msgBelowElement) {
        if(errorMessage){
            $j(msgBelowElement).parent().parent().after($j('<div class="error-msg single-col">'+errorMessage+'</div>'));
        }            
    }
    
    //Remove the element from the invalid element array
    function removeElementFromInvalidElementsArray(elementToRemove){
        var index = inValidElements.indexOf(elementToRemove);
        if (index > -1) {
            inValidElements.splice(index, 1);
        }
    }
    // Remove the error message
    function removeErrorMessage(msgElement){
        $j(msgElement).remove();
    }
    
    // Close the popup box
    function closeModal(){
        
        if(currentPopedUp){
            currentPopedUp.value ='';
            currentPopedUp.focus();
            removeElementFromInvalidElementsArray(currentPopedUp);
            removeErrorMessage($j(currentPopedUp).parent().parent().next('.error-msg')); 
            currentPopedUp = '';                    
        }
        setAllowSubmission();
        $j('#msgModal').css("display", "none");
    }

    // Set the form submission status
    function setAllowSubmission(){
        if(inValidElements.length > 0){
            allowSubmit = false;
        }else{
            allowSubmit = true;
        }
    }
    function assigncheckBoxValue(funcName, checkBoxElement){
        console.log('funcName', funcName);
        console.log('Element :', checkBoxElement);
        if(funcName=='tobeIncentiveName'){
            tobeIncentiveName(checkBoxElement.checked);
        }
        
        //funcName(true); 
    }
    function handleNACheckbox(element) {
        if(element.checked){
            $j($j(element).parent().parent().children()[1]).val("None");
            $j($j(element).parent().parent().children()[1]).attr("style", "display:none");            
            $j($j(element).parent().parent().children()[2]).attr("style", "display:inline-block;width: 50%;");;
        }else{
            $j($j(element).parent().parent().children()[1]).val("");
            $j($j(element).parent().parent().children()[1]).attr("style", "display:inline-block");
            $j($j(element).parent().parent().children()[1]).focus();
            $j($j(element).parent().parent().children()[2]).attr("style", "display:none");;
        }
    }
