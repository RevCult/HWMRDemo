    <fields>
        <fullName>Due_for_Payment__c</fullName>
        <externalId>false</externalId>
        <formula>IF( (Today()- blng__DueDate__c ) &gt;=45 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=59,45, 
IF( (Today()- blng__DueDate__c ) &gt;=60 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=74,60, 
IF( (Today()- blng__DueDate__c ) &gt;=75 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=89,75, 
IF( (Today()- blng__DueDate__c ) &gt;=90 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=104,90, 
IF( (Today()- blng__DueDate__c ) &gt;=105 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=119,105, 
IF( (Today()- blng__DueDate__c ) &gt;=120 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=134,120, 
IF( (Today()- blng__DueDate__c ) &gt;=135 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=149,135, 
IF( (Today()- blng__DueDate__c ) &gt;=150 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=164,150, 
IF( (Today()- blng__DueDate__c ) &gt;=165 &amp;&amp; (Today()- blng__DueDate__c ) &lt;=179,165, null)))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Due for Payment</label>
        <precision>18</precision>
