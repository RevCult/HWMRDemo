    <fields>
        <fullName>Term_Length_Months__c</fullName>
        <externalId>false</externalId>
        <formula>(YEAR( Zuora__TermEndDate__c ) * 12 + MONTH(Zuora__TermEndDate__c)) +((IF(MONTH(Zuora__TermEndDate__c) == MONTH( Zuora__TermStartDate__c ) || DAY(Zuora__TermEndDate__c) == DAY(Zuora__TermStartDate__c), 0, 1)) - (YEAR(Zuora__TermStartDate__c) * 12 + MONTH(Zuora__TermStartDate__c)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Term Length (Months)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Term_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Zuora__Status__c == &quot;Cancelled&quot; &amp;&amp; Zuora__CancelledDate__c &gt;= TODAY(),
            &quot;Active&quot;, /* future dated cancellations */
            IF(Zuora__Status__c == &quot;Cancelled&quot;,&quot;Cancelled&quot;,
            IF(FC_One_Time_Charge_Count__c &gt; 0 &amp;&amp; FC_Recurring_Charge_Count__c == 0,
            IF(Zuora__TermStartDate__c &gt; TODAY(), &quot;Future&quot;,
            IF(FC_Latest_Charge_Effective_Date__c &lt; TODAY(), &quot;Expired&quot;,
            &quot;Active&quot;
            )
            ),
            IF(FC_Recurring_Charge_Count__c &gt; 0,
            IF(Zuora__TermStartDate__c &gt; TODAY(),
            &quot;Future&quot;,
            IF(TEXT(Zuora__TermSettingType__c) == &quot;TERMED&quot;,
            IF(Zuora__TermEndDate__c &lt; TODAY(), &quot;Expired&quot;, &quot;Active&quot;),
            Zuora__Status__c
            )
            ),
            Zuora__Status__c
            )
            )
            )
)</formula>
        <label>Term Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
