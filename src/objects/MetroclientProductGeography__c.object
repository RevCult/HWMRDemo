    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Indicates if the product-geography is currently available for sale.</description>
        <externalId>false</externalId>
        <inlineHelpText>Indicates if the product-geography is currently available for sale. Inactive product geographies will not be included in the product selector&apos;s tiered selections (when applicable).</inlineHelpText>
        <label>Active</label>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ExcludeFromPriceTier__c</fullName>
        <defaultValue>false</defaultValue>
        <description>When TRUE, exclude from Product Selector&apos;s price tier</description>
        <externalId>false</externalId>
        <inlineHelpText>Check when product geography should be excluded from the product selector&apos;s tiered selections because product geography has its own dedicated rate plans.</inlineHelpText>
        <label>Exclude From Price Tier</label>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
