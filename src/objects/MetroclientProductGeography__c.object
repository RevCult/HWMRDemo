    <fields>
        <fullName>ProductCatalogRecordId__c</fullName>
        <description>18 character version of the SFDC Id.  Used by Zuora to associate subscription rate plan charges / product rate plan charges with a product geography.</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(Id)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>18 character unique id used by Zuora as the &quot;Record Id&quot; that associates subscription rate plan charges / product rate plan charges with a product geography.</inlineHelpText>
        <label>Product Catalog Record Id</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
