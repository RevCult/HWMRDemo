    <listViews>
        <fullName>RRI_Recently_Created</fullName>
        <columns>ProductGeographyType__c</columns>
        <columns>NAME</columns>
        <columns>Active__c</columns>
        <columns>Geography__c</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CREATED_DATE</field>
            <operation>greaterOrEqual</operation>
            <value>7/1/2017 12:00 AM</value>
        </filters>
        <label>RRI Products (Recently Created)</label>
        <language>en_US</language>
    </listViews>
    <listViews>
        <fullName>United_States</fullName>
        <columns>ProductCatalogRecordId__c</columns>
        <columns>ProductGeographyType__c</columns>
