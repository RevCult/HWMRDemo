    <validationRules>
        <fullName>AB2__DropUdfsRequireIndependentDrop</fullName>
        <active>true</active>
        <description>If the UDF Object Type is Drop there must be an associated Independent Drop.</description>
        <errorConditionFormula>$Setup.AB2__AdBook_Settings__c.AB2__UseIndependentDrops__c  &amp;&amp;
ISPICKVAL(AB2__AdBookDynamicProperty__r.AB2__ObjectType__c, &apos;Drop&apos;) &amp;&amp;  AB2__IndependentDrop__c  == null</errorConditionFormula>
        <errorDisplayField>AB2__IndependentDrop__c</errorDisplayField>
        <errorMessage>UDF Value missing Independent Drop (AdBook Line Item) association</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AB2__DropUdfsRequireOliId</fullName>
        <active>true</active>
        <description>If the UDF Object Type is Drop there must be an associated OLI Id.
NOTE: If this is failing in test cases, check the AdBook_Settings__c.getOrgDefaults().UseIndependentDrops__c value</description>
        <errorConditionFormula>NOT($Setup.AB2__AdBook_Settings__c.AB2__UseIndependentDrops__c) &amp;&amp;
ISPICKVAL(AB2__AdBookDynamicProperty__r.AB2__ObjectType__c, &apos;Drop&apos;) &amp;&amp; AB2__DropRecordId__c == null</errorConditionFormula>
        <errorDisplayField>AB2__DropRecordId__c</errorDisplayField>
        <errorMessage>UDF Value missing Opportunity Line Item (Product) association</errorMessage>
    </validationRules>
