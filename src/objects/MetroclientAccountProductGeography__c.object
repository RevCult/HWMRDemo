    <fields>
        <fullName>Status__c</fullName>
        <description>Status can be Active, Future, Expired, On Hold</description>
        <externalId>false</externalId>
        <formula>IF(Deleted__c  = True, &apos;Expired&apos;, 
  IF(EndDate__c &lt;   TODAY(), &apos;Expired&apos;, 
    IF(OnHold__c = True, &apos;On Hold&apos;,
    IF(StartDate__c &gt;  TODAY() , &apos;Future&apos;,

&apos;Active&apos;
)
)
)
)</formula>
        <inlineHelpText>Active, Future, Expired, or On Hold</inlineHelpText>
        <label>Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
