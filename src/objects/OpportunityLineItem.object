        <fullName>WF_Project_ID__c</fullName>
        <externalId>false</externalId>
        <label>WF Project ID</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>WF_Project_Link__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK(&quot;https://hanleywood.my.workfront.com/project/view?ID=&quot; +  WF_Project_ID__c   , &quot;WF Project&quot;,&quot;_blank&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>WF Project Link</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>WF_Project_Name__c</fullName>
        <description>This field is used to control the project name in WF</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( DSM_Product_Name__c ),

  IF( !ISBLANK(Legacy_DSM_ID_Line_Item__c),
  Legacy_DSM_ID_Line_Item__c &amp; &quot; &quot; &amp; PricebookEntry.Product2.Name  &amp; &quot; - &quot; &amp; WF_Account_Name__c,
  HW_Line_Item_Id__c &amp; &quot; &quot; &amp; PricebookEntry.Product2.Name &amp; &quot; - &quot; &amp; WF_Account_Name__c)
,
  IF( !ISBLANK(Legacy_DSM_ID_Line_Item__c),
  Legacy_DSM_ID_Line_Item__c &amp; &quot; &quot; &amp; DSM_Product_Name__c &amp; &quot; - &quot; &amp; WF_Account_Name__c,
  HW_Line_Item_Id__c &amp; &quot; &quot; &amp; DSM_Product_Name__c &amp; &quot; - &quot; &amp; WF_Account_Name__c)
)</formula>
        <label>WF Project Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>WF_Trigger_Time_01__c</fullName>
        <description>used in WF to fire time dependent WF after 1 minutes</description>
        <externalId>false</externalId>
        <formula>Now()  -  0.04097222</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>WF Trigger Time 01</label>
        <required>false</required>
        <type>DateTime</type>
    </fields>
    <validationRules>
        <fullName>Enforce_End_Date_Range</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
Opportunity.RecordTypeId = &quot;01280000000BiIi&quot;,

OR (
HW_Line_Item_End_Date__c  &lt; Opportunity.HW_Start_Date__c ,
HW_Line_Item_End_Date__c &gt;  Opportunity.HW_End_Date__c 
)
)</errorConditionFormula>
        <errorMessage>The Line Item End Date must be within the Start and End Date on the Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Start_Date_Range</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
Opportunity.RecordTypeId = &quot;01280000000BiIi&quot;,

OR (
HW_Line_Item_Start_Date__c &lt; Opportunity.HW_Start_Date__c ,
HW_Line_Item_Start_Date__c &gt;  Opportunity.HW_End_Date__c 
)
)</errorConditionFormula>
        <errorMessage>The Line Item Start Date must be within the Start and End Date on the Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Maximum_Percent_Discount</fullName>
        <active>false</active>
        <description>Rep can only discount a product 10%, if more than 10%, Rep cannot save the product</description>
        <errorConditionFormula>UnitPrice &gt;0.10</errorConditionFormula>
        <errorMessage>Product discount cannot exceed 10%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Products_on_Media_Opps</fullName>
        <active>false</active>
        <errorConditionFormula>AND (OR(
Opportunity.RecordTypeId = &quot;01280000000BhaG&quot;,
Opportunity.RecordTypeId = &quot;01280000000BiIi&quot;),
NOT (ISBLANK ( Line_Item_ID__c )),
NOT ( $User.ProfileId =&quot;00e80000000tVFl&quot;),
OR (
ISCHANGED (  OpportunityId  ),
ISCHANGED (  Account_Name__c ),
ISCHANGED (  Product2Id ),
ISCHANGED (  Product_Description__c ),
ISCHANGED (  ListPrice ),
ISCHANGED (  Product_Type__c ),
ISCHANGED (  ServiceDate ),
ISCHANGED (   Ad_ID__c ),
ISCHANGED (  AdName_Comments__c ),
ISCHANGED (  Adv_Rate__c ),
ISCHANGED (  Adv_Rate_Base__c ),
ISCHANGED (  Cost__c ),
ISCHANGED (  Cost_Base__c ),
ISCHANGED (  Custom_ProdType__c ),
ISCHANGED (  GDFP_Ad_ID__c ),
ISCHANGED (  GDFP_LI_Comments__c ),
ISCHANGED (  GDFP_Size__c ),
ISCHANGED (  DSM_ProdID__c ),
ISCHANGED (  DSM_ProdName__c ),
ISCHANGED (  is_flat_fee__c ),
ISCHANGED (  LI_StartDate__c ),
ISCHANGED (  LI_EndDate__c ),
ISCHANGED (  Line_Item_ID__c ),
ISCHANGED (  Net_Cost__c ),
ISCHANGED (  Net_Cost_Base__c ),
ISCHANGED (  Rate_Method__c ),
ISCHANGED (  Sitename__c ),
ISCHANGED (  Sitename__c ),
ISCHANGED (  DFP_Sitename__c ),
ISCHANGED (  Size__c ),
ISCHANGED (  Units_Delivered__c ),
ISCHANGED (  Units_Purchased__c ),
ISCHANGED (  Description )



)


)</errorConditionFormula>
        <errorMessage>Line items can only be edited in DSM.</errorMessage>
    </validationRules>
